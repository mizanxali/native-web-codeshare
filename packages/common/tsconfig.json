{
    "compilerOptions": {
      "target": "es5",
      "lib": ["dom", "dom.iterable", "esnext"],
      "skipLibCheck": true,
      "strict": true,
      "forceConsistentCasingInFileNames": true ,
      "esModuleInterop": true,
      "module": "CommonJS",
      "outDir": "dist",
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "declaration": true,
      "jsx": "react",
      "incremental": true,
      "paths": {
        "@/*": ["./*"]
      }
    },
    "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx"],
    "exclude": ["node_modules"]
  }
   